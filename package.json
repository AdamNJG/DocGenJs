{
  "name": "doc-gen-js",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "doc-gen-js": "./dist/cli.mjs"
  },
  "exports": {
    "./config": {
      "types": "./dist/config.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "start": "node ./dist/cli.mjs",
    "build": "tsup src/Cli --out-dir dist/Cli --format cjs --dts --splitting true && npm run build:cli && npm run build:defaults",
    "build:cli": "cp src/cli.mjs dist/cli.mjs",
    "build:defaults": "rimraf dist/Defaults && cp -r src/Defaults dist/Defaults",
    "clean": "rimraf ./dist && rimraf ./docs && rimraf ./.stryker-tmp && rimraf ./reports && rimraf ./coverage",
    "test": "vitest run --coverage",
    "test:cli": "npm run build && npx vitest run --config vitest.integration.config.ts",
    "test:mutation": "npx stryker run && rimraf ./.stryker-tmp"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/types": "^7.28.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/jest-runner": "^9.1.1",
    "@stryker-mutator/vitest-runner": "^9.1.1",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/babel__traverse": "^7.28.0",
    "@types/jsdom": "^21.1.7",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.3.0",
    "@types/prismjs": "^1.26.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "execa": "^9.6.0",
    "jiti": "^2.5.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-resolve": "^2.5.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "jsdom": "^26.1.0",
    "micromatch": "^4.0.8",
    "prismjs": "^1.30.0",
    "prompts": "^2.4.2"
  }
}
